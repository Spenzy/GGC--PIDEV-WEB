    <link href={{asset("css/shop.css")}} rel="">

{% endblock %}
{% block title %}Livraison{% endblock %}

{% block body %}
    <h1>Livraison</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Idcommande</th>
                <td>{{ livraison.idcommande }}</td>
