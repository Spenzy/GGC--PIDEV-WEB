                <td>{{ livraison.dateheure ? livraison.dateheure|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_livraison_index') }}" class="btn btn-outline-info">retour vers la liste</a>


    {{ include('livraison/_delete_form.html.twig') }}
